# git commands
alias gfo='git fetch origin'
alias ggpull='git pull origin'
alias ggpush='git push origin'
alias gco='git checkout'
alias glg='git log --oneline -10 --pretty=format:"%h - %an (%ar) : %s"'
alias gll='git lga'
alias gcm='git commit -m'
alias gucm='git uncommit'
alias gust='git unstage'
alias gudo='git uncommithard'
alias gpr='git pull --rebase'
alias gb='git branch'
alias gba='git branch -a'
alias gsb='git status -s'
alias grb='git rebase'
